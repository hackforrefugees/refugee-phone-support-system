{
  "public": "public",
  "rewrites": [
    {"source": "/users/", "destination": "/index.html"},
    {"source": "/users/*", "destination": "/index.html"}
  ],
  "headers": [
    {
      "source" : "*",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]
    }
  ]
}
